            <ImageView fitHeight="77.0" fitWidth="147.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@remove.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnMaximizar" layoutX="327.0" layoutY="98.0" mnemonicParsing="false">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@maximize.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnFechar" layoutX="527.0" layoutY="98.0" mnemonicParsing="false">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@close.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
